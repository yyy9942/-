package vo;

public class AdverticeVO {
	private int adver_id;
	private int item_id;
	private int realtor_id;
	public int getAdver_id() {
		return adver_id;
	}
	public void setAdver_id(int adver_id) {
		this.adver_id = adver_id;
	}
	public int getItem_id() {
		return item_id;
	}
	public void setItem_id(int item_id) {
		this.item_id = item_id;
	}
	public int getRealtor_id() {
		return realtor_id;
	}
	public void setRealtor_id(int realtor_id) {
		this.realtor_id = realtor_id;
	}
	
}
