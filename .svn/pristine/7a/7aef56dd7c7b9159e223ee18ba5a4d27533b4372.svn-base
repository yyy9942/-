<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bpSiteInfoContainer" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="siteInfo.siteInfo.SiteInfoController">
   <top>
      <VBox fx:id="vbSiteInfo" alignment="TOP_CENTER" maxWidth="1600.0" minWidth="1600.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="문의하기">
               <font>
                  <Font size="36.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" top="30.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" maxWidth="1200.0" minWidth="1200.0">
               <VBox.margin>
                  <Insets bottom="30.0" top="30.0" />
               </VBox.margin>
               <children>
                  <JFXButton fx:id="btnFrequentQuestions" maxHeight="60.0" maxWidth="400.0" minHeight="60.0" minWidth="400.0" onAction="#onFrequentQuestions" style="-fx-background-color: #0080FF;" text="자주 묻는 질문" textFill="WHITE">
                     <font>
                        <Font name="휴먼고딕" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btn1to1Chat" maxHeight="60.0" maxWidth="400.0" minHeight="60.0" minWidth="400.0" onAction="#on1to1Chat" text="1:1 문의">
                     <font>
                        <Font name="휴먼고딕" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnNoticeBoard" maxHeight="60.0" maxWidth="400.0" minHeight="60.0" minWidth="400.0" onAction="#onNoticeBoard" text="공지사항">
                     <font>
                        <Font name="휴먼고딕" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="100.0" right="100.0" top="50.0" />
         </padding>
      </VBox>
   </top>
   <center>
      <VBox fx:id="vbSiteInfo" alignment="TOP_CENTER" prefHeight="500.0" BorderPane.alignment="TOP_CENTER">
         <padding>
            <Insets bottom="100.0" left="100.0" right="100.0" />
         </padding>
      </VBox>
   </center>
</BorderPane>
