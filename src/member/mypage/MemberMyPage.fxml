<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bpMypageContainer" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="member.mypage.MemberMyPageController">
   <top>
      <VBox alignment="TOP_CENTER" maxWidth="1600.0" minWidth="1600.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="My 니방내방">
               <font>
                  <Font size="36.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" top="30.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" maxWidth="1200.0" minWidth="1200.0">
               <VBox.margin>
                  <Insets bottom="30.0" top="30.0" />
               </VBox.margin>
               <children>
                  <JFXButton fx:id="btnEditInfo" maxHeight="60.0" maxWidth="400.0" minHeight="60.0" minWidth="400.0" onAction="#onEditInfoClick" style="-fx-background-color: #0080FF;" text="정보수정" textFill="WHITE">
                     <font>
                        <Font name="휴먼고딕" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnReceiveRealtor" maxHeight="60.0" maxWidth="400.0" minHeight="60.0" minWidth="400.0" onAction="#onReceiveClick" text="연락한 부동산">
                     <font>
                        <Font name="휴먼고딕" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnMyReview" maxHeight="60.0" maxWidth="400.0" minHeight="60.0" minWidth="400.0" onAction="#onMyReviewClick" text="내가 쓴 리뷰">
                     <font>
                        <Font name="휴먼고딕" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets left="100.0" right="100.0" top="50.0" />
         </padding>
      </VBox>
   </top>
</BorderPane>
